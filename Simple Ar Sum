using System;
class HelloWorld {
  static void Main() {
      
   
        int sonuc = 0;
        int a;
        a= Convert.ToInt32(Console.ReadLine());
        int[] arraycik = new int[a];
        
    

        for(int i= 0; i < a; i++)
        {
        arraycik[i]= Convert.ToInt32(Console.ReadLine());
        sonuc= arraycik[i]+sonuc;
        };
       Console.Write(sonuc);
      
  }
}
